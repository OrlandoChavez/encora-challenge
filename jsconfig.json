{
	"compilerOptions": {
		"module": "ES2022",
		"target": "ES2022",
		"lib": ["ES2022", "DOM"],
		"checkJs": true,
		"moduleResolution": "Node16",
		"alwaysStrict": true,
		"noImplicitAny": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUncheckedIndexedAccess": true,
		"strictNullChecks": true,
		"resolveJsonModule": true
	},
	"exclude": ["node_modules"]
}